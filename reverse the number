#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,reverse=0, digit,org_num;
    
    scanf("%d",&num);
    org_num=num;
    //To run while loop to reverse the number
    while (num!=0)
    {
        digit=num%10;
        reverse=reverse*10+digit;
        num=num/10;
    }
    
    if(reverse==org_num)
    printf("palindrome");
    else
        printf("not palindrome");
    return 0;
}
